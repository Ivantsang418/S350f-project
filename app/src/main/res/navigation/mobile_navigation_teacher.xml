<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation_teacher"
    app:startDestination="@+id/navigation_info_teacher">
    <fragment
        android:id="@+id/navigation_info_teacher"
        android:name="com.example.schoolapplication.ui.teacher.info.TeacherInfoFragment"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_info_teacher" />

    <fragment
        android:id="@+id/navigation_academic_teacher"
        android:name="com.example.schoolapplication.ui.student.academic.AcademicFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_teacher_academic" />

    <fragment
        android:id="@+id/navigation_document_teacher"
        android:name="com.example.schoolapplication.ui.student.document.DocumentFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_teacher_document" />
    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.example.schoolapplication.ui.student.notification.NotificationFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_teacher_notification" />
    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.schoolapplication.ui.student.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />


</navigation>